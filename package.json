{
    "name": "sn-tools",
    "version": "1.1.3",
    "scripts": {
        "dev": "vite",
        "build":"node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build --base=/sn-tools/",
        "serve": "vite preview",
        "push:gepage": "git subtree push --prefix=dist gitee ge-pages",
        "ext": "vue-tsc --noEmit && vite build",
        "push:ext": "git subtree push --prefix=dist gitee extensions"
    },
    "dependencies": {
        "element-plus": "^2.2.28",
        "jsbarcode": "^3.11.5",
        "pinia": "^2.0.16",
        "vue": "^3.2.20",
        "vue-clipboard3": "^2.0.0",
        "vue-router": "^4.1.1",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^17.0.23",
        "@vitejs/plugin-vue": "^1.9.3",
        "consola": "^2.15.3",
        "type-fest": "^2.16.0",
        "typescript": "^4.6.3",
        "unplugin-auto-import": "^0.9.2",
        "unplugin-vue-components": "^0.21.1",
        "vite": "^2.8.6",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-style-import": "^2.0.0",
        "vue-tsc": "^0.3.0"
    }
}
