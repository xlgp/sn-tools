{
    "name": "sn-tools",
    "version": "1.4.0",
    "scripts": {
        "dev": "vite",
        "build": "node --max_old_space_size=1024000 ./node_modules/vue-tsc/bin/vue-tsc.js --noEmit && ./node_modules/vite/bin/vite.js build --base=/sn-tools/",
        "serve": "vite preview",
        "push:gepage": "git subtree push --prefix=dist gitee ge-pages",
        "ext": "vue-tsc --noEmit && vite build",
        "push:ext": "git subtree push --prefix=dist gitee extensions"
    },
    "dependencies": {
        "element-plus": "^2.2.28",
        "jsbarcode": "^3.11.5",
        "pinia": "^2.0.28",
        "vue": "^3.2.45",
        "vue-clipboard3": "^2.0.0",
        "vue-router": "^4.1.6",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@babel/types": "^7.20.7",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^18.11.18",
        "@vitejs/plugin-vue": "^4.0.0",
        "consola": "^2.15.3",
        "terser": "^5.16.1",
        "type-fest": "^3.5.1",
        "typescript": "^4.9.4",
        "unplugin-auto-import": "^0.12.1",
        "unplugin-vue-components": "^0.22.12",
        "vite": "^4.0.4",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-style-import": "^2.0.0",
        "vue-tsc": "^1.0.24"
    }
}
